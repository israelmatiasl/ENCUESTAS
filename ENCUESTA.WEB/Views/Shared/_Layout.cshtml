@*@@model ENCUESTA.WEB.ViewModel.AccountViewModel.RegisterViewModel*@
@{ 
    ENCUESTA.WEB.Models.Usuario usuario = Session["objusuario"] as ENCUESTA.WEB.Models.Usuario;
    if(usuario!=null)
    {

    }
}
<!doctype html>
<html lang="en">
<!-- Mirrored from demos.creative-tim.com/material-kit-pro/index.html by HTTrack Website Copier/3.x [XR&CO'2014], Fri, 07 Jul 2017 02:57:52 GMT -->
<head>
    <meta charset="utf-8" />
    <link rel="apple-touch-icon" sizes="76x76" href="@Url.Content("~/Content/assets/img/apple-icon.png")">
    <link rel="icon" type="image/png" href="@Url.Content("~/Content/assets/img/favicon.png")">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>Recomendación de Pagos</title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <!-- Canonical SEO -->
    <link rel="canonical" href="http://www.creative-tim.com/product/material-kit-pro" />
    <!--     Fonts and icons     -->
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
    <link rel="stylesheet" href="@Url.Content("~/Content/assets/css/font-awesome.min.css")" />
    <!-- CSS Files -->
    <link href="@Url.Content("~/Content/assets/css/bootstrap.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/assets/css/material-kit.min3f71.css?v=1.1.1")" rel="stylesheet" />
    <!-- CSS Just for demo purpose, don't include it in your project -->
    <link href="@Url.Content("~/Content/assets/assets-for-demo/vertical-nav.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/assets/assets-for-demo/demo.css")" rel="stylesheet" />
</head>
<body class="index-page">
    <nav class="navbar navbar-danger navbar-fixed-top" id="sectionsNav">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#example-navbar-info">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @if (TempData["title"] as String == "indexhome")
                {
                    <a class="navbar-brand" href="@Url.Action("Encuesta", "Home")">ENCUÉSTATE!</a>
                }
                else
                {
                    <a class="navbar-brand" href="@Url.Action("Index", "Home")">REGRESAR INICIO</a>
                }
            </div>
            <div class="collapse navbar-collapse" id="example-navbar-info">
                <ul class="nav navbar-nav navbar-right">

                    @if (usuario != null)
                    {
                        <li><a href="@Url.Action("ShowResult", "Result")">Mi última encuesta</a></li>
                        <li><a href="@Url.Action("Index", "Result")">Mis últimas encuestas</a></li>
                    }

                    @if (usuario != null)
                    {
                        <li>

                            <a href="@Url.Action("SignOut", "Account")"> Salir</a>
                        </li>
                    }
                    else
                    {

                        <li class="active" style="padding-right:15px">
                            <button class="btn btn-danger" data-toggle="modal" data-target="#signupModal" >
                                Registrarse
                            </button>
                        </li>
                        <li>
                            <button class="btn btn-danger" data-toggle="modal" data-target="#loginModal">
                                Ingresar
                            </button>
                        </li>
                    }
                </ul>
            </div>
        </div>
    </nav>

        <div class="header-danger">
            @if (TempData["title"] as String == "indexhome")
            {

                <div class="page-header" style=" background-image: url('@Url.Content("~/Content/assets/img/bg26.jpg")');height: 100vh;">
                    <div class="container">
                        @RenderBody()
                    </div>
                </div>
            }
            else
            {

                <div class="page-header">
                    <div class="container">
                        @RenderBody()
                    </div>
                </div>
            }
        </div>


    <div class="modal fade" id="loginModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-login">
            <div class="modal-content">
                <div class="card card-signup card-plain">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true"><i class="material-icons">clear</i></button>
                        <div class="header header-danger text-center">
                            <h4 class="card-title">Iniciar Sesión</h4>
                        </div>
                    </div>
                    <div class="modal-body">
                        <form class="form" method="post" action="@Url.Action("Login", "Account")">
                            <div class="card-content">
                                <div class="input-group">
                                    <span class="input-group-addon">
                                        <i class="material-icons">email</i>
                                    </span>
                                    <input type="text" class="form-control" placeholder="Email..." name="emaillogin"/>
                                </div>
                                <div class="input-group">
                                    <span class="input-group-addon">
                                        <i class="material-icons">lock_outline</i>
                                    </span>
                                    <input type="password" class="form-control" placeholder="Contraseña..." name="passlogin" />
                                </div>
                            </div>
                            <div class="modal-footer text-center">
                                <button type="submit" class="btn btn-danger btn-simple btn-wd btn-lg">Ingresar</button>
                            </div>
                        </form>
                    </div>
                    
                </div>
            </div>
        </div>
    </div>


    <!-- Register Modal -->
    <div class="modal fade" id="signupModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-login">
            <div class="modal-content">
                <div class="card card-signup card-plain">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true"><i class="material-icons">clear</i></button>
                        <div class="header header-danger text-center">
                            <h4 class="card-title">Registro</h4>
                        </div>
                    </div>
                    <div class="modal-body">
                        <div class="row">
                            <div class="col-md-12">
                                <form class="form" method="post" action="@Url.Action("Register", "Account")">
                                    <div class="card-content">
                                        <div class="input-group">
                                            <span class="input-group-addon">
                                                <i class="material-icons">face</i>
                                            </span>
                                            <input type="text" class="form-control" placeholder="Nombres..." name="namereg" />
                                        </div>
                                        <div class="input-group">
                                            <span class="input-group-addon">
                                                <i class="material-icons">email</i>
                                            </span>
                                            <input type="text" class="form-control" placeholder="Email..." name="emailreg" />
                                        </div>
                                        <div class="input-group">
                                            <span class="input-group-addon">
                                                <i class="material-icons">lock_outline</i>
                                            </span>
                                            <input type="password" class="form-control" placeholder="Contraseña..." name="passreg" />
                                        </div>
                                    </div>
                                    <div class="modal-footer text-center">
                                        <button type="submit" class="btn btn-danger btn-simple btn-wd btn-lg">Registrarse</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--  End Modal -->

        <!--   Core JS Files   -->
        <script src="@Url.Content("~/Content/assets/js/jquery.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Content/assets/js/bootstrap.min.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Content/assets/js/material.min.js")"></script>
        <!--  Plugin for Date Time Picker and Full Calendar Plugin-->
        <script src="@Url.Content("~/Content/assets/js/moment.min.js")"></script>
        <!--	Plugin for the Sliders, full documentation here: http://refreshless.com/nouislider/ -->
        <script src="@Url.Content("~/Content/assets/js/nouislider.min.js")" type="text/javascript"></script>
        <!--	Plugin for the Datepicker, full documentation here: https://github.com/Eonasdan/bootstrap-datetimepicker -->
        <script src="@Url.Content("~/Content/assets/js/bootstrap-datetimepicker.js")" type="text/javascript"></script>
        <!--	Plugin for Select, full documentation here: http://silviomoreto.github.io/bootstrap-select -->
        <script src="@Url.Content("~/Content/assets/js/bootstrap-selectpicker.js")" type="text/javascript"></script>
        <!--	Plugin for Tags, full documentation here: http://xoxco.com/projects/code/tagsinput/  -->
        <script src="@Url.Content("~/Content/assets/js/bootstrap-tagsinput.js")"></script>
        <!--	Plugin for Fileupload, full documentation here: http://www.jasny.net/bootstrap/javascript/#fileinput -->
        <script src="@Url.Content("~/Content/assets/js/jasny-bootstrap.min.js")"></script>
        <script src="@Url.Content("~/Content/assets/js/atv-img-animation.js")" type="text/javascript"></script>
        <!-- Control Center for Material Kit: activating the ripples, parallax effects, scripts from the example pages etc -->
        <script src="@Url.Content("~/Content/assets/js/material-kit.min3f71.js?v=1.1.1")" type="text/javascript"></script>
        <!-- Fixed Sidebar Nav - JS For Demo Purpose, Don't Include it in your project -->
        <script src="@Url.Content("~/Content/assets/assets-for-demo/modernizr.js")" type="text/javascript"></script>
        <script src="@Url.Content("~/Content/assets/assets-for-demo/vertical-nav.js")" type="text/javascript"></script>

        @RenderSection("Scripts", false)
</body>

